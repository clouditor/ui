    "prettier-plugin-tailwindcss": "^0.5.10",
    "svelte": "^4.0.5",
    "svelte-check": "^3.6.2",
    "svelte-popperjs": "^1.3.2",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  },
  "type": "module",
  "overrides": {
		"@rgossiaux/svelte-headlessui": {
			"svelte": "$svelte"
		}
	}
